class Solution:
    def minOperations(self, nums: List[int]) -> int:
        res = 0
        for i in range(len(nums) -2):
            if not nums[i] and i + 1 < len(nums) and i + 2 < len(nums):
                nums[i] = abs(nums[i] - 1)
                nums[i + 1] = abs(nums[i + 1] - 1)
                nums[i + 2] = abs(nums[i + 2] - 1)
                res += 1
        if sum(nums) < 1 * len(nums):
            return -1
        else: return res
